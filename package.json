    "@types/node": "^17.0.40",
    "@types/rimraf": "^3.0.2",
    "esbuild": "^0.14.25",
    "jest": "^28.1.0",
    "rimraf": "^3.0.2",
    "ts-jest": "^28.0.4",
    "typescript": "^4.7.3"
  },
  "peerDependencies": {
    "esbuild": "^0.14.25"
  },
  "author": "Stefan Zweifel",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/stefanzweifel/esbuild-mix-manifest-plugin/issues"
