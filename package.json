{
  "name": "@stefanzweifel/esbuild-mix-manifest-plugin",
  "version": "0.2.0",
  "description": "esbuild plugin to generate a mix-manifest.json file inspired by Laravel Mix.",
  "main": "lib/index.js",
  "files": [
    "lib/**/*"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/stefanzweifel/esbuild-mix-manifest-plugin.git"
  },
  "scripts": {
    "build": "tsc",
    "build-esbuild": "esbuild src/index.ts --platform=node --outfile=lib/index.js",
    "build:watch": "tsc --watch",
    "test": "jest"
  },
  "keywords": [
    "esbuild",
    "plugin",
    "manifest",
    "laravel-mix"
  ],
  "dependencies": {
